<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">

    <SolidColorBrush x:Key="PrimaryBackground" Color="White"></SolidColorBrush>
    <SolidColorBrush x:Key="PrimaryForeground" Color="Black"></SolidColorBrush>


    <SolidColorBrush x:Key="MainBtnColor" Color="#ffff556d"></SolidColorBrush>
    <SolidColorBrush x:Key="BoxBtnColor" Color="#ffff8e9F"></SolidColorBrush>

    <SolidColorBrush x:Key="ButtonTextColor" Color="#fffcf4f2" Opacity="0.85"></SolidColorBrush>
    <FontWeight x:Key="ButtonTextWeight">DemiBold</FontWeight>
    
</ResourceDictionary>